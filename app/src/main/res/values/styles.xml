<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#23283a</item>
        <item name="colorPrimaryDark">#23283a</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="drawerArrowStyle">@style/MyDrawerArrowToggle</item>
    </style>

    <style name="edittextcolor" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorControlNormal">@color/colorPrimaryDark</item>
        <item name="colorControlActivated">@color/colorAccent</item>

    </style>

    <style name="edittextcolor." parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
    </style>

    <style name="sos" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#23283a</item>
        <item name="colorPrimaryDark">#23283a</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyDrawerArrowToggle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@color/White</item>
    </style>

    <style name="sos.AppBarOverlay">sos</style>

    <style name="AppTheme.PopupOverlay">sos</style>

</resources>
